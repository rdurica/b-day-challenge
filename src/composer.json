{
	"name": "rdurica/b-day-challange",
	"description": "Fun tasks & challenges for memorable birthday celebrations! Videos, images, quizzes & more!",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT"],
	"repositories": [
		{
			"url": "git@github.com:rdurica/core.git",
			"type": "git"
		}
	],
	"require": {
		"php": ">= 8.0",
		"rdurica/core": "^0.1.0",
		"tracy/tracy": "^2.9"
	},
	"require-dev": {
		"nette/tester": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
